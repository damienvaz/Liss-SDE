
program Factorial {

	declarations

		int1 = 2 -> integer;

		subprogram factorial(n -> integer) -> integer
		{
		  declarations
				res = 1 -> integer;

		  statements
				while (n > 0)
				{
					res = res * n;
					pred n ; //'pred n' or 'pred(n)' ??
				}

				return res;
		}

	statements

		int1 = factorial(3*int1);
		writeln(int1);
}
